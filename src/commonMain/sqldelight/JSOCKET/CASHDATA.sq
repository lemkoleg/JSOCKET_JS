 clear_CashData:
 DELETE FROM CashData;

 select_CashData_All_CASH_SUM_NUMBER_POSITION:
  SELECT  CASH_SUM, NUMBER_POSITION, OBJECT_ID, LAST_UPDATE
  FROM CashData
  ORDER BY CASH_SUM, NUMBER_POSITION;

 update_CashData_NUMBER_POSITION_LAST_UPATE:
   UPDATE CashData
   SET  NUMBER_POSITION = ?,
        LAST_UPDATE = ?
   WHERE CASH_SUM = ?
   AND OBJECT_ID = ?;

 select_CashData_CASH_SUM_NUMBER_POSITION:
   SELECT  CASH_SUM, NUMBER_POSITION, OBJECT_ID, LAST_UPDATE
   FROM CashData
   WHERE CASH_SUM = ?
   ORDER BY CASH_SUM, NUMBER_POSITION;

 select_CashData_WITH_NUMBER_POSITION_limit:
   SELECT  *
   FROM CashData
   WHERE CASH_SUM = ?
   AND NUMBER_POSITION < ?
   ORDER BY CASH_SUM, NUMBER_POSITION
   LIMIT (SELECT  VALUE_VALUE FROM MetaData WHERE VALUE_NAME = 'LIMIT_FOR_SELECT');

 select_CashData_all:
 SELECT  * FROM CashData
 ORDER BY CASH_SUM, NUMBER_POSITION;

 select_CashData_count:
 SELECT count(*) FROM CashData;
