 index_CashData_CASH_SUM_NUMBER_POSITION:
 CREATE INDEX ICashData_CASH_SUM_NUMBER_POSITION
 ON CashData(CASH_SUM, NUMBER_POSITION, OBJECT_ID,LAST_UPDATE);

 index_CashData_CASH_SUM_OBJECT_ID:
  CREATE INDEX ICashData_CASH_SUM_OBJECT_ID
  ON CashData(CASH_SUM, OBJECT_ID);

 trigger_CashData_deleting_records:
 CREATE TRIGGER TCashData_DeletingRecord
 AFTER
 UPDATE ON CashData
 FOR EACH ROW
 WHEN SUBSTR(new.STRING_20,7,1) = '1'
  BEGIN
    DELETE FROM CashData WHERE rowid = new.rowid;
 END;
       
