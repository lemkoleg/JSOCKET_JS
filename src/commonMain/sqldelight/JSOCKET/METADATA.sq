table_metadata:
CREATE TABLE IF NOT EXISTS MetaData
(VALUE_NAME TEXT PRIMARY KEY,
 VALUE_VALUE INTEGER  NOT NULL,
 STRING_20 TEXT);

insert_metadata:
REPLACE INTO MetaData (VALUE_NAME, VALUE_VALUE, STRING_20) VALUES (?, ?, ?);

clear_metadata:
DELETE FROM MetaData;

select_all_metadata:
SELECT * FROM MetaData;

select_metadata:
SELECT VALUE_VALUE FROM MetaData WHERE VALUE_NAME = ?;